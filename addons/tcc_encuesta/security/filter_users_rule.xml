<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="tcc_survey_communal_council_rule" model="ir.rule">
            <field name="name">Encuestas que el Consejo comunal puede ver</field>
            <field name="model_id" ref="survey.model_survey_survey"/>
            <field name="domain_force">[('consejocomunal_id.user_id','=',[user.id]),]</field>
            <field name="perm_read" eval='True'/>
            <field name="perm_write" eval='False'/>
            <field name="perm_create" eval='False'/>
            <field name="perm_unlink" eval='False'/>
            <field name="groups" eval="[(4,ref('tcc_noticias.group_tcc_consejo'))]"/>
        </record>
    </data>
</openerp>
